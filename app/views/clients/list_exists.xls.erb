<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
<Styles>
  <Style ss:ID="bb">
   <Font ss:Bold="1"/>
  </Style>
  <Style ss:ID="white">
   <Interior ss:Color="white" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="red">
   <Interior ss:Color="red" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="yellowlist">
   <Interior ss:Color="yellow" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="green">
   <Interior ss:Color="#2e8b57" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="endtd">
   <Interior ss:Color="#cadaba" ss:Pattern="Solid"/>
  </Style>
</Styles>

  <Worksheet ss:Name="Sheet1">
    <Table>
<Column ss:AutoFitWidth="1"/>
      <Row>
        <Cell ss:StyleID="bb"><Data ss:Type="String">Название</Data></Cell>
        <Cell ss:StyleID="bb"><Data ss:Type="String">Адрес</Data></Cell>
        <Cell ss:StyleID="bb"><Data ss:Type="String">Менеджер</Data></Cell>
        <Cell ss:StyleID="bb"><Data ss:Type="String">Контакт</Data></Cell>
      </Row>
    <% @clients.each do |row| 
      %>
      <Row>
        <Cell><Data ss:Type="String"><%= row.name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= row.address %></Data></Cell>
        <Cell><Data ss:Type="String"><%= row.manager %></Data></Cell>
        <Cell><Data ss:Type="String"><%= row.contact %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
