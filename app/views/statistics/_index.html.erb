<% @stats.each do |stat| %>
  <tr>
    <td><%= stat.id %></td>
    <td><%= stat.name %></td>
    <td><%= stat.score %></td>
    <td><%= stat.week %></td>
    <td><%= stat.auth.f_l_name %></td>
    <td><%= link_to 'Edit', edit_statistic_path(stat) %></td>
    <td><%= link_to 'Destroy', stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

